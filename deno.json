{
  "compilerOptions": {
    "lib": ["deno.ns", "deno.unstable"],
    "strict": true
  },
  "importMap": "import_map.json",
  "fmt": {
    "options": {
      "singleQuote": true,
      "lineWidth": 80,
      "semiColons": true,
      "useTabs": false,
      "indentWidth": 2,
      "proseWrap": "always"
    }
  }
}
